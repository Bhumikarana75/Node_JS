<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>View</title>
</head>

<body>

    <div align="center">
        <h1>View Page</h1>
        <form action="/viewUser">
            <table border="1">
                <thead>
                    <tr>
                        <td>Id</td>
                        <td>Name</td>
                        <td>Email</td>
                        <td>Phone</td>
                        <td>Gender</td>
                        <td>City</td>
                        <td>Hobby</td>
                        <td>Action</td>
                    </tr>
                </thead>
                <tbody>
                    <% key.map((val, id) => { %>
                        <tr>
                            <td>
                                <%= val.id %>
                            </td>
                            <td>
                                <%= val.userName %>
                            </td>
                            <td>
                                <%= val.userEmail %>
                            </td>
                            <td>
                                <%= val.userPhone %>
                            </td>
                            <td>
                                <%= val.userGender %>
                            </td>
                            <td>
                                <%= val.userCity %>
                            </td>
                            <td>
                                <%= val.userHobby %>
                            </td>
                            <td>
                                <a href="/deleteUser?deleteId=<%= val.id %>">Delete</a>
                                <a href="/editUser?editId=<%= val.id %>">Edit</a>
                            </td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        </form>
        <a href="/">add</a>
    </div>

</body>

</html>